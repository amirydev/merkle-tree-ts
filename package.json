{
  "name": "merkle-tree-ts",
  "version": "0.0.5",
  "description": "a simple typescript module for working with merkle trees",
  "main": "./lib/cjs/index.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "prebuild": "npm run clean",
    "clean": "rimraf lib && rimraf coverage",
    "build": "tsc -p tsconfig.cjs.json",
    "lint": "eslint . --ext .ts",
    "format": "run-script-os",
    "format:win32": "prettier --config .prettierrc \"./src/**/*.ts\" --write",
    "format:darwin:linux": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "format:default": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:dev": "jest --watchAll"
  },
  "keywords": [],
  "author": "Amir Yahalom <amirylm.dev@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/crypto-js": "^4.0.1",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.5",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">= 14.5"
  },
  "files": [
    "lib/"
  ],
  "dependencies": {
    "crypto-js": "^4.0.0"
  }
}
